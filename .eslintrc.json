{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    // "plugin:@typescript-eslint/recommended",
    "plugin:import/recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["@typescript-eslint", "import"],
  "rules": {
    "prefer-template": "warn",
    "import/order": ["error", { "alphabetize": { "order": "asc" } }]
  }
}
